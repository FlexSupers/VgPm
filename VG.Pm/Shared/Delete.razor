@inherits DeleteView

<MudDialog title="Delete?">
    <DialogActions>
        <MudButton OnClick="Delete" Variant="Variant.Filled" Color="Color.Success" Raised="true">
            Yes
        </MudButton>
        <MudButton OnClick="Cancel" Variant="Variant.Filled" Color="Color.Primary" Raised="true">
            No
        </MudButton>
    </DialogActions>
</MudDialog>
